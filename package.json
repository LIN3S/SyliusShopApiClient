{
  "name": "lin3s-sylius-shop-api-client",
  "version": "0.9.3",
  "author": "LIN3S",
  "license": "MIT",
  "description": "JavaScript client on top of SyliusShopApiPlugin to build integrations with ease.",
  "keywords": [
    "lin3s",
    "sylius",
    "shop",
    "api",
    "js",
    "client"
  ],
  "main": "dist/common/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/LIN3S/SyliusShopApiClient"
  },
  "bugs": {
    "url": "https://github.com/LIN3S/SyliusShopApiClient/issues"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "phoenix": "rimraf node_modules && yarn install",
    "prebuild": "rimraf dist",
    "eslint": "node_modules/.bin/eslint src/**/*.js",
    "cs": "yarn eslint",
    "common-bundle": "node_modules/.bin/babel --copy-files --out-dir dist/common src",
    "esm-bundle": "cp -R src/ dist/esm",
    "build": "yarn prebuild && mkdirp dist && yarn cs && yarn common-bundle && yarn esm-bundle"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.7.2",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-class-property": "^1.0.6",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.merge": "^4.6.1",
    "react-cookies": "^0.1.0",
    "uuid": "^3.2.1"
  }
}
